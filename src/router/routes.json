{
  "/": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Dashboard is available to authenticated users",
            "rule": "authenticated"
          }
        ]
      },
      "redirectOnDeny": "/login"
    }
  },
  "/rooms": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Rooms page requires authentication",
            "rule": "authenticated"
          }
        ]
      },
      "redirectOnDeny": "/login"
    }
  },
  "/reservations": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Reservations page requires authentication",
            "rule": "authenticated"
          }
        ]
      },
      "redirectOnDeny": "/login"
    }
  },
  "/guests": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Guests page requires authentication",
            "rule": "authenticated"
          }
        ]
      },
      "redirectOnDeny": "/login"
    }
  },
  "/housekeeping": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Housekeeping page requires authentication",
            "rule": "authenticated"
          }
        ]
      },
      "redirectOnDeny": "/login"
    }
  },
  "/reports": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Reports page requires authentication",
            "rule": "authenticated"
          }
        ]
      },
      "redirectOnDeny": "/login"
    }
  },
  "/login": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Login page is public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/signup": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Signup page is public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/callback": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Callback page is public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/error": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Error page is public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/prompt-password/:appId/:emailAddress/:provider": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Password prompt page is public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/reset-password/:appId/:fields": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Password reset page is public",
            "rule": "public"
          }
        ]
      }
    }
  }
}